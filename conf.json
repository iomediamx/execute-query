{
  "source": {
    "include": ["./"],
    "exclude": ["node_modules", "execute-query"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./",
    "package": "./package.json",
    "readme": "./README.md",
    "recurse": true
  }
}
